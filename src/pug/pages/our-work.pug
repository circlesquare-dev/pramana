block menuLinks
    - var menu1
    - var menu2 = 'active'
    - var menu3
    - var menu4

mixin owList(ow_title1, ow_info1, ow_title2, ow_info2, ow_title3, ow_info3, ow_title4, ow_info4)
    .ow_item.mn_slide
        ul
            li
                .ow_item-title
                    p= ow_title1
                .ow_item-info
                    p= ow_info1
            li
                .ow_item-title
                    p= ow_title2
                .ow_item-info
                    p= ow_info2
            li
                .ow_item-title
                    p= ow_title3
                .ow_item-info
                    p= ow_info3
            li
                .ow_item-title
                    p= ow_title4
                .ow_item-info
                    p= ow_info4

doctype html
html
    head
        title Pramana
        meta(charset='utf-8')
        meta(name='viewport', content='initial-scale=1.0, width=device-width')
        meta(name='theme-color', content='#ffffff')

        link(type='text/css' rel='stylesheet' href='https://uploads-ssl.webflow.com/5dad6ad8f43e6fccc0e88db2/css/pramana.webflow.93f757c37.css')
        link(type='text/css' rel='stylesheet' href='styles/grid.min.css')
        link(type='text/css' rel='stylesheet' href='styles/styles.min.css')

    body
        .preloader
            .preloader__ico
                svg.svg_animation(fill='none', height='69', viewBox='0 0 119 99', width='119', xmlns='http://www.w3.org/2000/svg')
                    g(stroke='#092290', stroke-width='2')
                        path(d='m18 59.5c0 4.6944-3.8056 8.5-8.5 8.5-4.69442 0-8.5-3.8056-8.5-8.5s3.80558-8.5 8.5-8.5c4.6944 0 8.5 3.8056 8.5 8.5z', fill='#092290')
                        path(d='m68 9.5c0 4.6944-3.8056 8.5-8.5 8.5s-8.5-3.8056-8.5-8.5c0-4.69442 3.8056-8.5 8.5-8.5s8.5 3.80558 8.5 8.5z', fill='#092290')
                        path(d='m118 59.5c0 4.6944-3.806 8.5-8.5 8.5s-8.5-3.8056-8.5-8.5 3.806-8.5 8.5-8.5 8.5 3.8056 8.5 8.5z')


        main
            include ../sections/main/header

            include ../sections/our-work/screen-dots
            include ../sections/our-work/main-cover
            include ../sections/our-work/fresh-start
            include ../sections/our-work/next-chapter
            include ../sections/our-work/new-direction
            include ../sections/our-work/cta

            include ../sections/main/footer

        script(src='https://d3e54v103j8qbb.cloudfront.net/js/jquery-3.4.1.min.220afd743d.js')
        script(src='https://cdnjs.cloudflare.com/ajax/libs/gsap/1.20.3/TweenMax.min.js')
